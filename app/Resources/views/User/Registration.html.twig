{% extends "content.html.twig" %}

{% block title %}
    <h1>{{ 'user.registration_code'|trans({}, 'FitbaseUserBundle') }}</h1>
{% endblock title %}

{% block breadcrumbs %}
    <div class="crumbs">
        <div class="container">
            <ul>
                <li>{{ 'user.registration_code'|trans({}, 'FitbaseUserBundle') }}</li>
            </ul>
        </div>
    </div>
{% endblock breadcrumbs %}

{% block content %}
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <!-- Content Text-->
            <div class="panel-box">
                {% include 'User/RegistrationInternal.html.twig' with {'form': form} %}
            </div>
            <!-- End Content Text-->
        </div>
    </div>
{% endblock content %}