{% extends 'SonataPageBundle::demo_layout.html.twig' %}

{% block sonata_page_javascripts %}
    {{ parent() }}
        {% javascripts
        '@FitbaseFitbaseBundle/Resources/public/js/*'
        '@FitbaseQuestionnaireBundle/Resources/public/js/*'
        '@FitbaseExerciseBundle/Resources/public/js/*'
        output='js/fitbase.js' %}
    {% endjavascripts %}
{% endblock %}

{% block sonata_page_stylesheets %}
    {{ parent() }}
    {% stylesheets
    '@FitbaseFitbaseBundle/Resources/public/css/*'
    '@FitbaseQuestionnaireBundle/Resources/public/css/*'
    '@FitbaseExerciseBundle/Resources/public/css/*'
    output='css/fitbase.css' %}
    {% endstylesheets %}
{% endblock %}

