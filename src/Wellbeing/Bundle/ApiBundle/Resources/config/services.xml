    <imports>
        <import resource="controllers.xml"/>
        <import resource="admin.xml"/>
    </imports>

    <services>

        <service id="wellbeing.authentication" class="Wellbeing\Bundle\ApiBundle\Service\ServiceUserAuthenticationKey">
