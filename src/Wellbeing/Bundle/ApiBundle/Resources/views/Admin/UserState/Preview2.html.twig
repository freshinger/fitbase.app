{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}

    <table>
        <tbody>
        <tr>
            <td colspan="2">
                {% thumbnail object.preview2, 'admin' with {'height': 100} %}
            </td>
        </tr>
        <tr>
            <th>Sh:</th>
            <td>{{ object.squareXZShoulder }}</td>
        </tr>
        <tr>
            <th>L.Sh./Sp:</th>
            <td>{{ object.squareXZShoulderLeftSpine }}</td>
        </tr>
        <tr>
            <th>R.Sh./Sp:</th>
            <td>{{ object.squareXZShoulderRightSpine }}</td>
        </tr>
        </tbody>
    </table>

{% endblock %}
