<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <imports>
        <import resource="admin.xml" />
        <import resource="blocks.xml" />
        <import resource="subscribers.xml" />
        <import resource="helpers.xml"/>
    </imports>

    <services>

        <service id="wellbeing.ergonomics" class="Wellbeing\Bundle\ErgonomicsBundle\Service\ServiceErgonomics">
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

    </services>
</container>
