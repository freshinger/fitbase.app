<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <services>
        <service id="wellbeing.ergonomics.user" class="Wellbeing\Bundle\ErgonomicsBundle\Admin\UserErgonomicsAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Wellbeing" label="Ergonomics"/>
            <argument/>
            <argument>Wellbeing\Bundle\ErgonomicsBundle\Entity\UserErgonomics</argument>
            <argument/>
        </service>
        <service id="wellbeing.ergonomics.user_state" class="Wellbeing\Bundle\ErgonomicsBundle\Admin\UserStateErgonomicsAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Wellbeing" label="Ergonomics states"/>
            <argument/>
            <argument>Wellbeing\Bundle\ErgonomicsBundle\Entity\UserStateErgonomics</argument>
            <argument/>
        </service>
        <service id="wellbeing.ergonomics.settings" class="Wellbeing\Bundle\ErgonomicsBundle\Admin\UserErgonomicsSettingsAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Wellbeing" label="Ergonomics settings"/>
            <argument/>
            <argument>Wellbeing\Bundle\ErgonomicsBundle\Entity\UserErgonomicsSettings</argument>
            <argument/>
        </service>
    </services>

</container>
