<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Wellbeing\Bundle\ExerciseBundle\Entity\UserStateExerciseNeck">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <one-to-one field="userState" target-entity="Wellbeing\Bundle\ExerciseBundle\Entity\UserStateExercise">
            <join-column name="userStateId" referenced-column-name="id" on-delete="CASCADE"/>
        </one-to-one>

    </entity>
</doctrine-mapping>
