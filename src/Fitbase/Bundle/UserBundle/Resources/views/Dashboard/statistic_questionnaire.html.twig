<div class="panel panel-default">
    <div class="panel-body">
        <h4 class="short">Die Auswertung der Mitglieder</h4>

        <div class="row">
            <div class="col-md-6">
                <h4>Allgemeiner Gesundheitszustand</h4>

                <p style="height: 150px; line-height: 21px; font-size: 13px;">
                    {{ health|recommendation1 }}
                </p>

                <div class="circular-bar center">
                    <input data-displayinput="false" value="{{ health|health }}"
                           data-linecap="round" class="knob"
                           style="display: none;">

                    <div class="circular-bar-content">
                        <strong>0%</strong>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <h4>Belastungsprofil</h4>

                <p style="height: 150px; line-height: 21px; font-size: 13px;">
                    {{ strain|recommendation2 }}
                </p>

                <div class="circular-bar center">
                    <input data-displayinput="false" data-readonly="true"
                           value="{{ strain|strain }}"
                           data-linecap="round" class="knob" style="display: none;" readonly="readonly">

                    <div class="circular-bar-content">
                        <strong>0%</strong>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>