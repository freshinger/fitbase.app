{% block sonata_page_javascripts %}
    {% javascripts
    '@FitbaseGamificationBundle/Resources/public/js/*'
    '@FitbaseGamificationBundle/Resources/public/ctl/*' output='js/jquery.gamification.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block sonata_page_stylesheets %}
    {% stylesheets '@FitbaseGamificationBundle/Resources/public/css/*' output='css/jquery.gamification.css' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}

{% block content %}
{% endblock %}