<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="gamification_picture_avatar" path="/picture/avatar">
        <default key="_controller">FitbaseGamificationBundle:Picture:avatar</default>
    </route>

    <route id="gamification_picture_tree" path="/picture/tree">
        <default key="_controller">FitbaseGamificationBundle:Picture:tree</default>
    </route>

    <route id="gamification_picture_forest" path="/picture/forest">
        <default key="_controller">FitbaseGamificationBundle:Picture:forest</default>
    </route>


    <route id="gamification_health_chat" path="/health/chat">
        <default key="_controller">FitbaseGamificationBundle:Gamification:healthChat</default>
    </route>

    <route id="gamification_health_emotion" path="/health/emotion">
        <default key="_controller">FitbaseGamificationBundle:Gamification:healthEmotion</default>
    </route>

</routes>
