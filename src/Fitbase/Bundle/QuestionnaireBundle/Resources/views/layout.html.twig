{% extends 'SonataPageBundle::demo_layout.html.twig' %}

{% block sonata_page_javascripts %}
    {{ parent() }}
    {% javascripts '@FitbaseQuestionnaireBundle/Resources/public/js/*' output='js/jquery.questionnaire.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block sonata_page_stylesheets %}
    {{ parent() }}
    {% stylesheets '@FitbaseQuestionnaireBundle/Resources/public/css/flick/*' output='css/jquery.questionnaire.css' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}


{% block content %}
    {{ parent() }}
{% endblock %}